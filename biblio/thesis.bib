  booktitle = {{{IEEE China Summit}} and {{International Conference}} on {{Signal}} and {{Information Processing}} ({{ChinaSIP}}'2015)},
  author = {Wu, Gang and Hu, Su and Li, Shaoqian},
  date = {2015-07},
  pages = {1002--1006},
  location = {{Chengdu, China}},
  doi = {10.1109/ChinaSIP.2015.7230555},
  abstract = {As an overlay cognitive radio, transform domain communication system (TDCS) has been proposed to support reliable communications with low probability of interception. However, most of research on TDCS assume perfect synchronization to simplify algorithm design. In this paper, we present a low complexity symbol timing offset (STO) and carrier frequency offset (CFO) estimation method for practical TDCS applications. By utilizing preamble consisted of multiple identical training sequences, STO can be estimated by the demodulation procedure of cyclic code shift keying (CCSK), in which the circular correlation is performed by utilizing fast Fourier transform (FFT). Thus the estimation procedure is significantly simplified with considerable complexity reduction, which is analyzed and compared to other two conventional methods. The simulation results demonstrates that the proposed STO and CFO estimation scheme is a low complexity solution for TDCS in multipath Rayleigh fading channels with comparable estimation performance in terms of probability of incorrect timing offset and mean-square-error of CFO.},
  eventtitle = {2015 {{IEEE China Summit}} and {{International Conference}} on {{Signal}} and {{Information Processing}} ({{ChinaSIP}})},
  keywords = {carrier frequency offset,Cognitive radio,Communication systems,Complexity theory,Correlation,Estimation,Fading,synchronization,Timing,timing offset,transform domain communication system,Transforms}
}

@article{wyglinskiRevolutionizingSoftwareDefined2016,
  title = {Revolutionizing Software Defined Radio: Case Studies in Hardware, Software, and Education},
  shorttitle = {Revolutionizing Software Defined Radio},
  author = {Wyglinski, Alexander M. and Orofino, Don P. and Ettus, Matthew N. and Rondeau, Thomas W.},
