  keywords = {Binary erasure channel (BEC),capacity,Channel capacity,Cities and towns,Communication system control,complexity,Computer simulation,degree distribution (d.d.),density evolution (DE),H infinity control,irregular repeat–accumulate (IRA) codes,iterative decoding,Iterative decoding,Iterative methods,low-density parity-check (LDPC) codes,Materials science and technology,Parity check codes,Product codes,systematic codes}
}

